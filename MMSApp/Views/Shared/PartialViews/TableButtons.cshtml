<a asp-action="Edit" data-toggle="tooltip" data-placement="top" title="ویرایش" asp-route-id="@Model" type="button" class="btn btn-icon btn-yellow"><i class="fa-solid fa-pen"></i></a>
<a asp-action="Details" data-toggle="tooltip" data-placement="top" title="مشاهده جزئیات" asp-route-id="@Model" type="button" class="btn btn-icon  btn-success"><i class="fa-solid fa-eye"></i></a>
<a asp-action="Delete" data-toggle="tooltip" data-placement="top" title="حذف ریکورد" asp-route-id="@Model" type="button" class="btn btn-icon  btn-danger"><i class="fe fe-trash"></i></a>

