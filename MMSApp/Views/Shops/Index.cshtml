            <th>
                @Html.DisplayNameFor(model => model.BusinesslicenseImage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RedirectUrl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShopLogo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustomerKey)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustomerValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShopNameFa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShopNameEn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CityCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.taxPayerCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsDeleted)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatorUserId)
